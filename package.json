{
  "name": "coding-insight",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start-en": "docusaurus start --locale en",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-en": "docusaurus write-translations --locale en",
    "write-heading-ids": "docusaurus write-heading-ids",
    "prettier": "prettier --write .",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "format:diff": "prettier --config .prettierrc --list-different \"**/*.{js,jsx,ts,tsx,md,mdx}\""
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@algolia/client-search": "^4.9.1",
    "@docusaurus/core": "^2.0.1",
    "@docusaurus/plugin-ideal-image": "^2.0.1",
    "@docusaurus/plugin-pwa": "^2.0.1",
    "@docusaurus/preset-classic": "^2.0.1",
    "@fontsource/rubik": "^4.5.11",
    "@mdx-js/react": "^1.6.22",
    "classnames": "^2.3.1",
    "docusaurus-plugin-sass": "^0.2.2",
    "mdx-mermaid": "^1.3.0",
    "mermaid": "9.1.4",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-quiz-component": "^0.3.9",
    "sass": "^1.52.3"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.1"
  }
}
